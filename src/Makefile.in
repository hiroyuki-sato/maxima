@GCL_TRUE@@GCL_ALT_LINK_FALSE@	test -d binary-gcl/numerical/slatec || mkdir binary-gcl/numerical/slatec
@GCL_TRUE@@GCL_ALT_LINK_FALSE@	LISPTYPE=gcl ; export LISPTYPE ;\
@GCL_TRUE@@GCL_ALT_LINK_FALSE@	GCL=$(GCL_NAME) ; export GCL ;\
@GCL_TRUE@@GCL_ALT_LINK_FALSE@	$(RUNLISP) \
@GCL_TRUE@@GCL_ALT_LINK_FALSE@        -x '(load "$(top_srcdir)/lisp-utils/defsystem.lisp")(funcall (intern "OPERATE-ON-SYSTEM" :mk) "maxima" :compile :verbose t)' && \
@GCL_TRUE@@GCL_ALT_LINK_FALSE@        $(RUNLISP) \
@GCL_TRUE@@GCL_ALT_LINK_FALSE@        -x '(load "$(top_srcdir)/lisp-utils/defsystem.lisp")(funcall (intern "OPERATE-ON-SYSTEM" :mk) "maxima" :load :verbose t)' \
@GCL_TRUE@@GCL_ALT_LINK_FALSE@         -d binary-gcl/maxima

@GCL_TRUE@install-gcl:
@GCL_TRUE@	$(mkinstalldirs) "$(DESTDIR)$(verpkglibdir)/binary-gcl"
@GCL_TRUE@	$(INSTALL_PROGRAM) binary-gcl/maxima "$(DESTDIR)$(verpkglibdir)/binary-gcl/maxima"

@GCL_TRUE@uninstall-gcl:
